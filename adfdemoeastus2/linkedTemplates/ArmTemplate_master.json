{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adfdemoeastus2"
		},
		"l_adfsqlserver001_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'l_adfsqlserver001'"
		},
		"l_myadfstorage0001_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'l_myadfstorage0001'"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adfdemoeastus2_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"l_adfsqlserver001_connectionString": {
						"value": "[parameters('l_adfsqlserver001_connectionString')]"
					},
					"l_myadfstorage0001_connectionString": {
						"value": "[parameters('l_myadfstorage0001_connectionString')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}